<%- include ("../_partials/_header.ejs"); %>
 
<h1>Catalog</h1>
 
<% post.forEach(function(post) { %>
    <div class="row"><div class="col-sm-6 col-md-offset-3">
        <div class="thumbnail">
            <% if(post.image != null){%>
                <img src="<%= "/" + post.image %>"
                    alt="logo"
                    style="height:200px;width:px;display:block">
            <%}else{%>
                <img src="data:image/svg+xml;base64,"
                    alt="logo"
                    style="height:200px;width:px;display:block">
            <%}%>
            <div class="caption">
                <h3><%= post.title %></h3>
                <p><%= post.description %></p>
                <p>$<%= post.price %> per ticket</p>
                <p><%= post.actors %></p>
                <p><%= post.listedtimes %>pm</p>
                <p><a href="/catalog/<%=post._id%>" class="btn btn-primary" role="button">Read</a> 
                <p><a href="catalog/cart/<%= post._id %>" class="btn btn-primary" role="button">Add to Cart</a>
            </div>
        </div>
    </div>
<% }) %>
</div>
    
<%- include ("../_partials/_footer.ejs"); %>