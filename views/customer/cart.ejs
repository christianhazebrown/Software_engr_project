<%- include ("../_partials/_header.ejs"); %>
 
<h1>Cart</h1>
<% display.forEach(function(display){ %>
    <div class="row"><div class="col-sm-6 col-md-offset-3">
        <div class="thumbnail">
            <% if(display.image != null){%>
                <img src="<%= "/" + display.image %>"
                    alt="logo"
                    style="height:200px;width:px;display:block">
            <%}else{%>
                <img src="data:image/svg+xml;base64,"
                    alt="logo"
                    style="height:200px;width:px;display:block">
            <%}%>
            <div class="caption">
                <h3><%= display.title %></h3>
                <p><%= display.description %></p>
                <p>$<%= display.price %> per ticket</p>
                <p><%= display.actors %></p>
                <p><%= display.listedtimes %>pm</p>
            </div>
        </div>
    </div>
<% }) %>
</div>
<style>
    p{
        text-align: center;
    }
</style>
<p><a href="/" class="btn btn-primary" role="button">Checkout</a> 

